let graduadosDeHTML5 =  "30 45 25 34 18 23 16 50 72 70".split(" ") 
let graduadosDeCSS3 ="50 45 71 34 23 45 65 75 63 43 74 70".split(" ") 
let graduadosDeJAVASCRIPT = "70 65 58 45 23 57 34 17 72".split(" ") 
let graduadosDeNODEJS = "45 56 73 34 65 72 70 32".split(" ") 

let obtenerPromedio = (gDeHTML5,gDeCSS3,gDeJAVASCRIPT,gDeNODEJS,categorias) => {

    if(categorias > 4 || categorias < 1 ) {

        let mensaje = ' los cursos asignados son: HTML5 CSS3 JAVASCRIPT NODEJS '
        return mensaje ;
    
}

let categorias = [null,gDeHTML5,gDeCSS3,gDeJAVASCRIPT,gDeNODEJS]
    let categoriaSeleccionada = categorias[categoria]
    let sumaValores = 0 ;
    for (let i = 0; 1 < categoriaSeleccionada.length;i++){

        let valorVenta = +categoriaSeleccionada[i]
        sumaValores += valorVenta
    }
    let cantidadVenta = categoriaSeleccionada.length
    return sumaValores / cantidadVenta
}

let total = obtenerPromedioVentas(graduadosDeHTML5,graduadosDeCSS3, 
graduadosDeJAVASCRIPT,graduadosDeNODEJS) ;

